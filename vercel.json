{
    "version": 2,
    "builds": [
        {
            "src": "real_time_detection/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "real_time_detection/wsgi.py"
        }
    ]
}
